#
# ~/.bashrc
#

# If not running interactively, don't do anything
[[ $- != *i* ]] && return

alias ls='ls --color=auto'
PS1='[\u@\h \W]\$ '
alias autoclick=~/gitmess/xautoclick-0.31/gautoclick2
alias monitorfix="xrandr --output DisplayPort-1 --mode 1920x1080 --rate 144"
alias blackice="~/Games/Black_Ice"
alias launch_moc="st -c music_on_console -e mocp &"
alias update_discord="exec ~/scripts/discordinstall"
